@{
    ViewBag.Title = "Home Page";
}


<!--<form id="Formulario">-->
<form action="/Home/RecibirDatos" method="post">
<div id="Formulario">
    <div class="form-group row">
        <label for="colFormLabel" class="col-sm-2 col-form-label">Código de cliente:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  name="codeClient"    id="codeClient" value="xmxwebdemo2" disabled>
        </div>
    </div>
    <div class="form-group row">
        <label for="colFormLabel" class="col-sm-2 col-form-label">Usuario:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  name="Usuario"     id="Usuario" placeholder="">
        </div>
    </div>
    <div class="form-group row">
        <label for="colFormLabel" class="col-sm-2 col-form-label">Nombre:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  name="name"    id="name" placeholder="Nombre:">
        </div>
    </div>
    <div class="form-group row">
        <label for="colFormLabel" class="col-sm-2 col-form-label">Cargo:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"   name="cargo"    id="cargo" placeholder="Cargo:">
        </div>
    </div>
    <div class="form-group">
        <div class="form-group row">
            <label for="colFormLabel" class="col-sm-2 col-form-label">Telefono:</label>
            <div class="input-group-prepend" style="width: 50px;">
                <div class="input-group-text">+57</div>
            </div>
            <input type="tel" class="form-control" style="width: 250px"  name="Telefono"    id="Telefono" placeholder="Telefono:">
        </div>
    </div>
    <div class="form-group row">
        <label for="colFormLabel" class="col-sm-2 col-form-label">Correo Electrónico:</label>
        <div class="col-sm-10">
            <input type="email" class="form-control"   name="Email"   id="Email" placeholder="Correo Electrónico:">
        </div>
    </div>
    <div class="form-group row">
        <label for="colFormLabel" class="col-sm-2 col-form-label">Numero de celular:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" name="NCelular" id="NCelular" placeholder="Numero celular:">
        </div>
    </div>
    <div class="form-group row">
        <label for="colFormLabel"  id="Tcontact" class="col-sm-2 col-form-label">Tipo de contacto</label>
        <div class="col-sm-10">
            <select class="form-control" name="Tcontact">
                <option>--Seleccione tipo de contacto--</option>
                <option>Contacto Comercial</option>
                <option>Pago de factura</option>
                <option>Representante legal</option>
                <option>Retiro de mercadería</option>
            </select>
        </div>
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Autorizado para acceder a webstore</label>
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Autorizado para crear ordenes</label>
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">¿Desea que se envie la informacion de acceso al usuario?</label>
    </div>
    &nbsp;
    <div class="alert alert-danger" id="Error" style="display:none" role="alert">
        This is a danger alert—check it out!
    </div>
    <div class="alert alert-success" id="Success" style="display:none" role="alert">
        El formulario es exitoso
    </div>
    &nbsp;
    <div style="text-align: center;">
        <button type="submit" class="btn btn-primary mb-2S" id="btnGenerate">Aceptar</button>
        <button type="submit" class="btn btn-primary mb-2S">Cancelar</button>
    </div>
</div>
</form>






